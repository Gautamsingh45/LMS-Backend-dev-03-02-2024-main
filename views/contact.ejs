<style>
   .logo {
      display: flex;
      align-items: center;
      text-decoration: none;
    }
    
    .large-logo {
      max-width: 100%;
      max-height: 85px; /* Set your preferred maximum height */
      height: auto;
    }
   .logo .small-logo{
      
      width: 112px;
      height: 85px;
  }
   .logo img{
      width: 100%;
      height: 100%;
      border-radius: 20px;
   }
   @media screen and (max-width: 1600px) {
      .logo .large-logo {
          display: block; /* Hide the large logo on small screens */
      }
      .logo .small-logo {
          display: none; /* Show the small logo on small screens */
      }
  }
  @media screen and (max-width: 600px) {
      .logo .large-logo {
          display: none; /* Hide the large logo on small screens */
      }
      .logo .small-logo {
          display: block; /* Show the small logo on small screens */
      }
  }



  
  .footer2 {
    
   color: #b34892;
   padding: 40px 60px;
