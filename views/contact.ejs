 }
 
 /* Center the content in the container */
 .content {
   max-width: 1200px;
   margin: 0 auto;
 }
 
 /* Style the top section containing logo and social media icons */
 .top {
   display: flex;
   justify-content: space-between;
   align-items: center;
 }
 
 /* Style the logo details */
 .logo-details {
   display: flex;
   align-items: center;
 }
 
 .logo-details i {
   font-size: 24px;
   margin-right: 5px;
 }
 
 .logo_name {
   font-size: 20px;
 }
 
 /* Style the media icons */
 .media-icons a {
   color: #2e2b2b;
   margin-right: 15px;
   text-decoration: none;
   font-size: 18px;
 }
 
 /* Style the link boxes containing lists of links */
 .link-boxes {
   display: flex;
   justify-content: space-between;
   margin-top: 20px;
 }
 
 .box {
   list-style: none;
 }
 
 .link_name {
   font-size: 18px;
   font-weight: bold;
   margin-bottom: 10px;
 }
 
 .box a {
   color: #222121;
   text-decoration: none;
   display: block;
   margin-bottom: 5px;
 }
 
 /* Style the input box for subscription */
 .input-box input[type="text"] {
   width: 100%;
   padding: 8px;
 }
 
 .input-box input[type="button"] {
   background-color: #e8491d;
   color: #2e2b2b;
   border: none;
   padding: 8px 15px;
   cursor: pointer;
 }
 
 /* Style the bottom details section */
 .bottom-details {
   margin-top: 20px;
 }
 
 .bottom_text {
   display: flex;
   justify-content: space-between;
   align-items: center;
 }
 
 .copyright_text a {
   color: #2e2b2b;
   text-decoration: none;
 }
 
 .policy_terms a {
   color: #1d1b1b;
   text-decoration: none;
   margin-left: 15px;
 }

 .cloudlab-logo {
   width: 240px; /* Set the desired width */
   height: auto; /* Maintain aspect ratio */
   margin-right: 10px; /* Adjust spacing */
 }
</style>
</head>
